{
  "name": "brisp/livewire-component-requests",
  "description": "A with the same interface as laravel form requests but then for livewire components.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Huub Verbeek",
      "email": "h.verbeek@brisp.nl",
      "role": "developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "BRISP\\LivewireComponentRequests\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BRISP\\LivewireComponentRequests\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LivewireComponentRequestsServiceProvider"
      ]
    }
  },
  "require": {
    "livewire/livewire": "^2.10"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.5",
    "laravel/pint": "^1.1"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter",
    "pint": "vendor/bin/pint",
    "pint-t": "vendor/bin/pint --test"
  }
}
